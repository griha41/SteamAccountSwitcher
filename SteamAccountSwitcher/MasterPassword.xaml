<Window x:Class="SteamAccountSwitcher.MasterPassword"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SteamAccountSwitcher"
        mc:Ignorable="d"
        Title="MasterPassword" Height="107.799" Width="300">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="10,34,0,0" TextWrapping="Wrap" Text="Master Password" VerticalAlignment="Top" Width="235"/>
        <Button x:Name="button" Content="Go" HorizontalAlignment="Left" Margin="250,34,0,0" VerticalAlignment="Top" Width="32" Height="23" Click="button_Click"/>
        <Label x:Name="label" Content="Please enter your Master Password:" HorizontalAlignment="Left" Margin="10,3,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.638,0.12"/>

    </Grid>
</Window>
